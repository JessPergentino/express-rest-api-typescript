    "test:watch": "jest --detectOpenHandles --forceExit --watchAll --no-cache --coverage",
    "build": "tsc",
    "lint": "eslint src/**",
    "apidoc": "apidoc -i src -o  public/apidoc"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "pino": "^8.8.0",
    "pino-http": "^8.3.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^2.5.0",
    "sequelize-mock": "^0.10.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.2.2"
  },
  "type": "module",
  "apidoc": {
    "name": "Documentação da API express-rest-api-typescript",
    "version": "1.0.0",
    "description": "Documentação inicial para o boilerplate de uma REST APIs usando Node.js e TypeScript."
