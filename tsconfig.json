{
  "compilerOptions": {
    "target": "es5",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "rootDir": "./",
    "baseUrl": "./src",
    "paths": {
      "@api/*": ["api/*"],
      "@config/*": ["config/*"],
      "@container/*": ["container/*"],
      "@middlewares/*": ["middlewares/*"],
      "@resource/*": ["resource/*"],
      "@routes/*": ["routes/*"],
      "@utils/*": ["utils/*"]
    },
    "resolveJsonModule": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  }
}
